{
    "tags": {
      "allowUnknownTags": ["component"],
      "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
      "include": ["src"],
      "includePattern": "\\.(jsx|js|ts|tsx)$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown","node_modules/better-docs/typescript", "node_modules/better-docs/component"],
    
    "templates": {
      "better-docs": {
        "name": "MultiPlayer - MyRadio"
      }
    },
    "opts": {
      "encoding": "utf8",
      "template": "node_modules/better-docs",
      "destination": "docs",
      "verbose": true,
      "recurse": true,
      "readme": "README.md"
    }
  }